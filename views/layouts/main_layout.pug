script.
  document.getElementById("n-2").addEventListener("mouseenter", enter);
  document.getElementById("n-2").addEventListener("mouseleave", leave);
  function leave()
  {
  document.getElementById("accordionPanelsStayOpenExample").style.display = "block";
  } 
  function enter()
  {
  document.getElementById("accordionPanelsStayOpenExample").style.display = "none";
  }
